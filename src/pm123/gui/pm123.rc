    MENUITEM "~Pause\tPad 5",              BMP_PAUSE
    MENUITEM "Fast ~forward",              BMP_FWD
    MENUITEM "Fast ~backward",             BMP_REW
    MENUITEM "~Next",                      BMP_NEXT
    MENUITEM "Pre~vious",                  BMP_PREV
