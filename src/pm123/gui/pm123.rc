  MENUITEM "~Save Stream...",              IDM_M_SAVE
  MENUITEM "~Float on Top",                IDM_M_FLOAT
  MENUITEM "~Hide Window",                 IDM_M_MINIMIZE
  MENUITEM SEPARATOR
  SUBMENU  "~Help",                        IDM_M_HELP
  { MENUITEM "General ~Help",              IDH_MAIN, MIS_HELP
    MENUITEM "Help for ~Main Window",      IDH_MAIN_WINDOW, MIS_HELP
    MENUITEM "~Keys Help",                 IDH_KEYBOARD, MIS_HELP
  }
  MENUITEM "~Quit",                        BMP_POWER
}

