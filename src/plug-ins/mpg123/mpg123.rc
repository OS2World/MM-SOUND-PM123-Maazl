#include <os2.h>
#include "mpg123.h"

DLGTEMPLATE DLG_CONFIGURE
BEGIN
  DIALOG "MP3 Decoder", 100, -75, -20, 270, 160,
          FS_MOUSEALIGN, FCF_TITLEBAR | FCF_SYSMENU | FCF_CLOSEBUTTON
  BEGIN
    CONTROL " Decode ", ID_NULL, 5, 130, 260, 26, WC_STATIC,
