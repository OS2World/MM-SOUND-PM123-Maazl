      CBS_DROPDOWNLIST | WS_GROUP | WS_TABSTOP | WS_VISIBLE


    CONTROL "Write tags", ID_NULL, 140, 107, 120, 8, WC_STATIC,
      SS_TEXT | DT_LEFT | DT_VCENTER | DT_MNEMONIC | WS_VISIBLE
